<article class="post">
	<%- partial('_partial/_post/header',{post: page}) %>
	<%- partial('_partial/_post/katex',{post: page}) %>
	<div>
		<% if (page.excerpt && page.hide_excerpt ){ %>
			<%- page.more %>
		<%} else{%>
			<%- page.content %>
		<%}%>
	</div>
	<%- partial('_partial/_post/footer',{post: page}) %>
</article>